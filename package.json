{
  "name": "@rokkit.ts/rokkit.ts-class-declaration-resolver",
  "version": "0.2.0",
  "description": "Rokkit.ts Class declaration resolver is used to create class information that are not present on runtime. It is mainly used by the rokkit.ts-dependency-injection package.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "build": "tsc --build tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rokkit-ts/rokkit.ts-class-declaration-resolver.git"
  },
  "keywords": [
    "rokkit.ts"
  ],
  "author": "JDeterling",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rokkit-ts/rokkit.ts-class-declaration-resolver/issues"
  },
  "homepage": "https://github.com/rokkit-ts/rokkit.ts-class-declaration-resolver#readme",
  "dependencies": {
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/fs-extra": "^8.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.3",
    "chai": "^4.2.0",
    "husky": "^3.0.5",
    "mocha": "^6.2.0",
    "mocha-typescript": "^1.1.17",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
